  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:status": "sequelize db:migrate:status",
    "migrate:undo": "sequelize db:migrate:undo",
    "dev": "dotenv -v NODENV=development -v PORT=4000 nodemon app.js",
    "start": "dotenv -v NODENV=production yarn nodemon app.js ",
    "drop:db": "sequelize db:drop",
    "recreate:db": "yarn drop:db && yarn create:db && yarn migrate",
    "create:db": "sequelize db:create"
  },
